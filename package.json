{
  "name": "prismflow-browser",
  "version": "1.0.0",
  "description": "The Browser That Breathes - Powered by Natural Asymmetry",
  "author": "PrismFlow Team",
  "main": "browser-stable.js",
  "scripts": {
    "start": "electron browser-stable.js --enable-features=SharedArrayBuffer",
    "native": "electron browser-native.js",
    "enhanced": "electron browser-enhanced.js",
    "optimized": "electron browser-optimizations.js",
    "vite-dev": "vite",
    "build-react": "vite build",
    "preview": "vite preview",
    "build": "npm run build-react && electron-builder",
    "build-win": "npm run build-react && electron-builder --win",
    "build-mac": "npm run build-react && electron-builder --mac",
    "build-linux": "npm run build-react && electron-builder --linux",
    "package-win": "electron-packager . prismflow-browser --platform=win32 --arch=x64 --out=dist-packager --overwrite",
    "dev": "electron browser-stable.js --dev --enable-features=SharedArrayBuffer",
    "test": "playwright test",
    "test:integration": "playwright test __tests__/integration --project=chromium",
    "test:headed": "playwright test --headed",
    "test:debug": "playwright test --debug",
    "test:report": "playwright show-report",
    "test:old": "electron test-all-features.js",
    "typecheck": "tsc --noEmit",
    "lint": "eslint ."
  },
  "devDependencies": {
    "@playwright/test": "^1.56.0",
    "@types/electron": "^1.4.38",
    "@types/jest": "^29.0.0",
    "@types/node": "^24.7.2",
    "@types/react": "^18.3.26",
    "@types/react-dom": "^18.3.7",
    "@vitejs/plugin-react": "^5.0.4",
    "autoprefixer": "^10.4.21",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "electron": "^32.3.3",
    "electron-builder": "^24.9.1",
    "electron-packager": "^17.1.2",
    "eslint": "^9.37.0",
    "jest": "^29.0.0",
    "lucide-react": "^0.545.0",
    "postcss": "^8.5.6",
    "puppeteer": "^21.0.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "sonner": "^2.0.7",
    "tailwind-merge": "^3.3.1",
    "tailwindcss": "^3.4.18",
    "tailwindcss-animate": "^1.0.7",
    "typescript": "^5.9.3",
    "vite": "^5.4.20"
  },
  "build": {
    "appId": "com.prismflow.browser",
    "productName": "PrismFlow Browser",
    "directories": {
      "output": "dist"
    },
    "files": [
      "browser-stable.js",
      "src/**/*",
      "!src/test*",
      "!test*",
      "!**/*.test.js"
    ],
    "win": {
      "target": [
        "nsis",
        "portable"
      ],
      "icon": "assets/icon.ico",
      "forceCodeSigning": false,
      "signAndEditExecutable": false,
      "verifyUpdateCodeSignature": false
    },
    "nsis": {
      "oneClick": false,
      "perMachine": true,
      "allowToChangeInstallationDirectory": true
    },
    "mac": {
      "target": "dmg",
      "icon": "assets/icon.icns",
      "category": "public.app-category.productivity"
    },
    "linux": {
      "target": [
        "AppImage",
        "deb"
      ],
      "icon": "assets/icon.png",
      "category": "Network"
    }
  },
  "dependencies": {
    "@radix-ui/react-accordion": "^1.2.12",
    "@radix-ui/react-alert-dialog": "^1.1.15",
    "@radix-ui/react-aspect-ratio": "^1.1.7",
    "@radix-ui/react-avatar": "^1.1.10",
    "@radix-ui/react-checkbox": "^1.3.3",
    "@radix-ui/react-collapsible": "^1.1.12",
    "@radix-ui/react-context-menu": "^2.2.16",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-hover-card": "^1.1.15",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-menubar": "^1.1.16",
    "@radix-ui/react-navigation-menu": "^1.2.14",
    "@radix-ui/react-popover": "^1.1.15",
    "@radix-ui/react-progress": "^1.1.7",
    "@radix-ui/react-radio-group": "^1.3.8",
    "@radix-ui/react-scroll-area": "^1.2.10",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-separator": "^1.1.7",
    "@radix-ui/react-slider": "^1.3.6",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-switch": "^1.2.6",
    "@radix-ui/react-tabs": "^1.1.13",
    "@radix-ui/react-toggle": "^1.1.10",
    "@radix-ui/react-toggle-group": "^1.1.11",
    "@radix-ui/react-tooltip": "^1.2.8"
  }
}
